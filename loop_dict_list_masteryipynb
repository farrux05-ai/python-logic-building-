    "jami = []\n",
    "for talaba in database:\n",
    "    if talaba['ball'] is not None:\n",
    "        jami.append(talaba['ball'])\n",
    "\n",
    "ortacha = sum(jami) / len(jami)\n",
    "print(f\"O'rtacha ball: {ortacha}\")\n",
    "# faol talabalarning o'rtacha balini hisoblash\n",
    "jami = 0\n",
    "faol_t = 0\n",
    "for student in database:\n",
    "    if student['faol']:\n",
    "        jami += student['ball']\n",
    "        faol_t +=1\n",
    "if jami > 0:\n",
    "    ortacha = jami/faol_t\n",
    "print(f\"Faol talabalarning o'rtcha bahosi: {ortacha:.2f}\")\n",
