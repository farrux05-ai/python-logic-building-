   "source": [
    "for item in data:\n",
    "    try:\n",
    "        ism = item['ism']\n",
    "        yosh = item['yosh']\n",
    "\n",
    "        if yosh is None:\n",
    "            print(f\"{ism} ni yoshi haqida ma'lumotlar yo'q\")\n",
    "        else:\n",
    "            yosh_int = int(yosh)\n",
    "            print(f\"{ism} 10 yildan keyin {yosh_int+10} yoshda bo'ladi\")\n",
    "    except KeyError as e:\n",
    "        print(f\" Xatolik {e} kaliti topilmadi - {item}\")\n",
    "    except ValueError as e:\n",
    "        print(f\"Yoshni raqamga aylantirib bo'lmadi: {e}\")\n",
