    "aralash_data = [\n",
    "    \"Ali\",           # string\n",
    "    25,              # int\n",
    "    5000000.5,       # float\n",
    "    True,            # boolean\n",
    "    None,            # None\n",
    "    [1, 2, 3],       # lista\n",
    "    {\"yosh\": 30}     # dictionary\n",
    "]\n",
    "aralash_data\n",
    "for item in aralash_data:\n",
    "    print(f\"Value item: {item}\")\n",
    "    print(f\"Type item: {type(item)}\")\n",
    "    print(f\"Name of item: {type(item).__name__}\")\n",
    "\n",
    "    #turini tekshirish\n",
    "    if isinstance(item, str):\n",
    "        print(f\"BU string: uzunligi: {len(item)}\")\n",
    "    elif isinstance(item, int):\n",
    "        print(f\"Bu butun son. Ikki barabar oshirsak {item *2}\")\n",
    "    elif isinstance(item,float):\n",
    "        print(f\"Bu o'nlik son. Yaxlitlasak: {round(item)}\")\n",
    "    elif isinstance(item, bool):\n",
    "        print(f\"Bu boolenan. {'ha' if item else 'yo\\'q'}\")\n",
    "    elif item == None:\n",
    "        print('Bosh qiymat')\n",
    "    elif isinstance(item, list):\n",
    "        print(f\"Bu ro'yxat. elementlar soni: {len(item)} ta\")\n",
    "    elif isinstance(item, dict):\n",
    "        print(f\"Bu lug'at.Qiymatlar soni: {len(item.keys())}\")\n",
    "        \n",
    "    print()"
   ]
